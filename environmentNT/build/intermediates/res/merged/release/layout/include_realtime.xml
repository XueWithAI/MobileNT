<?xml version="1.0" encoding="utf-8"?>
<com.sinoyd.environmentNT.hometouchview.TopSlidingLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/topSliding"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2.0">
        <include layout="@layout/include_real_time_new" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <include layout="@layout/include_real_time_down" />

    </RelativeLayout>






</com.sinoyd.environmentNT.hometouchview.TopSlidingLayout>




